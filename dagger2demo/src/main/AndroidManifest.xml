<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.nibiru.dagger2demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.nibiru.dagger2demo.component_depend.DependActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.nibiru.dagger2demo.MainActivity"/>
        <activity android:name="com.nibiru.dagger2demo.singleton_scope.SecondActivity"/>
        <activity android:name="com.nibiru.dagger2demo.Qualifiers_name.ThreeActivity"/>
        <activity android:name="com.nibiru.dagger2demo.provides_lazy.LazyActivity"/>
    </application>

</manifest>